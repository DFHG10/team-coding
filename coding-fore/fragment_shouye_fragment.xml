<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#FAFFC5"
            tools:context=".MainActivity">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="9dp"
                android:background="#FAFFC5"
                tools:context=".MainActivity">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="110dp"
                    android:layout_marginLeft="-80dp"
                    android:src="@drawable/huoguo" />

                <Button
                    android:id="@+id/to_huoguo"
                    android:layout_width="match_parent"
                    android:layout_height="110dp"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:text="                                   火锅"
                    android:textSize="24dp" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="2dip"
                    android:layout_marginTop="110dp"
                    android:background="#BBBBBB" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="110dp"
                    android:layout_marginLeft="-110dp"
                    android:layout_marginTop="110dp"
                    android:src="@drawable/baozi" />

                <Button
                    android:id="@+id/to_baozi"
                    android:layout_width="match_parent"
                    android:layout_height="110dp"
                    android:layout_marginTop="110dp"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:text="                                   包子馒头"
                    android:textSize="24dp" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="2dip"
                    android:layout_marginTop="220dp"
                    android:background="#BBBBBB" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="110dp"
                    android:layout_marginLeft="-158dp"
                    android:layout_marginTop="220dp"
                    android:src="@drawable/naicha" />

                <Button
                    android:id="@+id/to_naicha"
                    android:layout_width="match_parent"
                    android:layout_height="110dp"
                    android:layout_marginTop="220dp"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:text="                                   奶茶甜品"
                    android:textSize="24dp" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="2dip"
                    android:layout_marginTop="330dp"
                    android:background="#BBBBBB" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="110dp"
                    android:layout_marginLeft="-85dp"
                    android:layout_marginTop="330dp"
                    android:src="@drawable/mian" />

                <Button
                    android:id="@+id/to_noodle"
                    android:layout_width="match_parent"
                    android:layout_height="110dp"
                    android:layout_marginTop="330dp"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:text="                                   面馆"
                    android:textSize="24dp" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="2dip"
                    android:layout_marginTop="440dp"
                    android:background="#BBBBBB" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="110dp"
                    android:layout_marginLeft="-208dp"
                    android:layout_marginTop="440dp"
                    android:src="@drawable/kuaican" />

                <Button
                    android:id="@+id/to_fastfood"
                    android:layout_width="match_parent"
                    android:layout_height="110dp"
                    android:layout_marginTop="440dp"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:text="                                   快餐"
                    android:textSize="24dp" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="2dip"
                    android:layout_marginTop="550dp"
                    android:background="#BBBBBB" />

                <ImageView
                    android:layout_width="182dp"
                    android:layout_height="96dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="560dp"
                    android:src="@drawable/hanbao" />

                <Button
                    android:id="@+id/to_hanbao"
                    android:layout_width="match_parent"
                    android:layout_height="110dp"
                    android:layout_marginTop="550dp"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:text="                                   汉堡披萨"
                    android:textSize="24dp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="110dp"
                    android:layout_marginLeft="-220dp"
                    android:layout_marginTop="770dp"
                    android:src="@drawable/qita" />


                <View
                    android:layout_width="fill_parent"
                    android:layout_height="2dip"
                    android:layout_marginTop="660dp"
                    android:background="#BBBBBB" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="110dp"
                    android:layout_marginLeft="-208dp"
                    android:layout_marginTop="660dp"
                    android:src="@drawable/shaokao" />

                <Button
                    android:id="@+id/to_shaokao"
                    android:layout_width="match_parent"
                    android:layout_height="110dp"
                    android:layout_marginTop="660dp"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:text="                                   烧烤烤串"
                    android:textSize="24dp" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="2dip"
                    android:layout_marginTop="770dp"
                    android:background="#BBBBBB" />


                <Button
                    android:id="@+id/to_else"
                    android:layout_width="match_parent"
                    android:layout_height="110dp"
                    android:layout_marginTop="770dp"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:text="                                   其他"
                    android:textSize="24dp" />

                <LinearLayout
                    android:id="@+id/LinearLayout1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:background="#FAFFC5"
                    >
                </LinearLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="3dip"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="117dp"
                    android:background="#000000" />


            </RelativeLayout>



        </RelativeLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>